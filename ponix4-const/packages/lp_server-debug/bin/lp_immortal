#!/bin/sh
echo "!!!!!!!!!!!!!!!!!!Before cycle before sleep" >> /var/log/boot.log

sleep 10

echo "!!!!!!!!!!!!!!!!!!Before cycle after sleep" >> /var/log/boot.log

while [ 1 = 1 ]; do

echo "!!!!!!!!!!!!!!!!!!In cycle before first sleep" >> /var/log/boot.log

sleep 10

echo "!!!!!!!!!!!!!!!!!!In cycle before second sleep" >> /var/log/boot.log

sleep 10

#lp_server $1 $2 $3 $4 $5 $6 $7 $8 $9 2> /dev/null

echo "!!!!!!!!!!!!!!!!!!In cycle after sleep" >> /var/log/boot.log
done

echo "!!!!!!!!!!!!!!!!!!After cycle" >> /var/log/boot.log
