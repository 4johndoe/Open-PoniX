#!/bin/sh
echo "!!!!!!!!!!!!!!!!!!Before cycle start" >> /var/log/boot.log

while [ 1 = 1 ]; do

echo "!!!!!!!!!!!!!!!!!!In cycle before lp_server start" >> /var/log/boot.log

lp_server $1 $2 $3 $4 $5 $6 $7 $8 $9 2> /dev/null

echo "!!!!!!!!!!!!!!!!!!In cycle after lp_server start" >> /var/log/boot.log
done
