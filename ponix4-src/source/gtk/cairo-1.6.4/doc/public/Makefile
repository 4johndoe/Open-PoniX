Headers.mk:
	( echo "PRIVATE_TEST_HFILES = \\"; \
	find $(top_srcdir)/src \
		-name '*-private.h' -o \
		-name '*-test.h' | \
	sed 's@.*/@	@; s@$$@ \\@' | \
	LANG=C sort; \
	echo '	cairoint.h' ) > $@.tmp
	mv $@.tmp $@

