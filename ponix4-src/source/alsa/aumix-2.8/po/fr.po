msgstr "  C:  spécifier le modèle de couleurs\n"

#: src/common.c:701
msgid ""
"  d:  adjust a device besides /dev/mixer\n"
"  f:  specify file for saving and loading settings (defaults to\n"
"      ~/.aumixrc or /etc/aumixrc)\n"
"  h:  this helpful message\n"
msgstr ""
"  d:  ajuster un périphérique autre que /dev/mixer\n"
"  f:  spécifier le fichier de sauvegarde/chargement des paramètres\n"
"      (par défaut il s'agit de ~/.aumixrc ou /etc/aumixrc)\n"
"  h:  ce message d'aide\n"

#: src/common.c:707
msgid "  I:  start in interactive mode after doing non-interactive functions\n"
msgstr ""
"  I:  démarrer en mode interactif après les fonctions non-interactives\n"

#: src/common.c:710
msgid ""
"  L:  load settings\n"
"  q:  query all channels and print their settings\n"
"  S:  save settings\n"
msgstr ""
"  L:  charger les paramètres\n"
"  q:  imprimer les paramètres de tous les canaux\n"
"  S:  enregistrer les paramètres\n"

