#: src/common.c:656
msgid ""
"aumix %s usage: aumix [-<channel option>[[+|-][<amount>]]|<level>|\n"
"R[ecord]|P[lay]|q[uery]]"
msgstr ""
"aumix %s modo de emprego: aumix [-<opción de "
"canle>[[+|-][<cantidade>]]|<nivel>|\n"
"R[tocar]|P[tocar]|q[consultar]]"

