#: src/common.c:672
#, c-format
msgid ""
"aumix %s usage: aumix [-<channel option>[[+|-][<amount>]]|<level>|\n"
"R[ecord]|P[lay]|q[uery]] [-dhILqS] [-f <rc file>][-C <color scheme file>]\n"
"\n"
msgstr ""
"aumix %s modo de emprego: aumix [-<opción de canle>[[+|-][<cantidade>]]|<nivel>|\n"
"R[tocar]|P[tocar]|q[consultar]] [-dhILqS] [-f <rc file>][-C <esquema>]\n"
"\n"

#: src/common.c:674
#, c-format
msgid ""
"aumix %s usage: aumix [-<channel option>[[+|-][<amount>]]|<level>|\n"
"R[ecord]|P[lay]|q[uery]] [-dhLqS] [-f <rc file>]\n"
"\n"
msgstr ""
"aumix %s modo de emprego: aumix [-<opción de canle>[[+|-][<cantidade>]]|<nivel>|\n"
"R[tocar]|P[tocar]|q[consultar]] [-dhLqS] [-f <rc file>]\n"
"\n"

#: src/common.c:676
msgid ""
"channel options:\n"
"  v:  main volume           x:  mix monitor\n"
"  b:  bass                  W:  PCM 2\n"
"  t:  treble                r:  record\n"
"  s:  synthesizer           i:  input gain\n"
"  w:  PCM                   o:  output gain\n"
"  p:  PC speaker            1:  line 1\n"
"  l:  line                  2:  line 2\n"
"  m:  microphone            3:  line 3\n"
"  c:  CD\n"
"\n"
"other options:\n"
"  d:  adjust a device besides /dev/mixer\n"
"  f:  specify file for saving and loading settings (defaults to\n"
"      ~/.aumixrc or /etc/aumixrc)\n"
"  C:  specify color scheme\n"
"  h:  this helpful message\n"
msgstr ""
"opcións dos canais:\n"
"  v:  volume principal      x:  monitor do mix\n"
"  b:  graves                W:  PCM 2\n"
"  t:  agudos                r:  gravar\n"
"  s:  sintetizador          i:  gaño de entrada\n"
"  w:  PCM                   o:  gaño de saída\n"
"  p:  altofalante do PC     1:  liña 1\n"
"  l:  entrada de liña       2:  liña 3\n"
"  m:  micrófono             3:  liña 3\n"
"  c:  CD\n"
"\n"
"outras opcións:\n"
"  d:  axustar outro dispositivo que non sexa /dev/mixer\n"
"  f:  especificar ficheiro para gardar e cargar parámetros (por omisión é\n"
"      ~/.aumixrc ou /etc/aumixrc)\n"
"  C:  especifica-lo esquema de cor\n"
"  h:  amosar esta mensaxe de axuda\n"

#: src/common.c:694
msgid "  I:  start in interactive mode after doing non-interactive functions\n"
msgstr "  I:  comezar en modo interactivo tralas funcións non interactivas\n"

#: src/common.c:697
msgid ""
"  L:  load settings\n"
"  q:  query all channels and print their settings\n"
"  S:  save settings\n"
msgstr ""
"  L:  carga-los parámetros\n"
"  q:  escribi-la información de tódalas canles\n"
"  S:  garda-los parámetros actuais\n"

