prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Pango Xft
Description: Xft font support for Pango
Version: 1.4.0
Requires: pango
Libs: -L${libdir} -lpangoxft-1.0 -L/usr/xorg/lib -lXft -lXrender -lfontconfig -lfreetype -lz -lX11  
Cflags: -I${includedir}/pango-1.0 -I/usr/xorg/include -I/usr/include/freetype2  
